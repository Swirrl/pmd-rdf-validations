    pmdcat:graph <http://test/dataset/label/0> ;
    pmdcat:datasetContents <http://test/dataset/label/0> .

<http://test/dataset/label/2> a pmdcat:Dataset ;
    rdfs:label "I have two different labels" ;
    rdfs:label "I have two labels that are not the same" ;
    dct:title "DatasetExactlyOneLabel 2" ;
    dct:issued "yesterday" ;
    dct:modified "yesterday" ;
    pmdcat:graph <http://test/dataset/label/2> ;
    pmdcat:datasetContents <http://test/dataset/label/2> .


